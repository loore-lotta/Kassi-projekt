def m√§ngi_taustamuusikat():
    fs, data = read('taustamuusika.wav')
    while True:
        sd.play(data, fs)
        sd.wait()
        
muusika_l√µim = threading.Thread(target=m√§ngi_taustamuusikat, daemon=True)
muusika_l√µim.start()



